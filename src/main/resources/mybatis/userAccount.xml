<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dingpw.hornbook.dao.user.account.AccountDao">

  <insert id="insert" parameterType="com.dingpw.hornbook.dao.user.account.AccountEntity">
    INSERT INTO user
    (status,create_time,update_time)
    values
    (#{status},#{createTime},#{updateTime});
  </insert>

</mapper>